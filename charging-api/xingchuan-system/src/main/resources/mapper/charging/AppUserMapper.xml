<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xingchuan.charging.mapper.AppUserMapper">

    <select id="getInfoByOpenId" resultType="com.xingchuan.common.core.domain.entity.AppUser">
        select *
        from app_user
        where open_id = #{openId}
          and del_flag = '0'
    </select>
</mapper>