<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xingchuan.charging.mapper.ChargingStationReviewsMapper">

    <select id="getEvaluation" resultType="com.xingchuan.charging.domain.model.EvaluationModel">
        select rating as "value", count(1) as qty
        from charging_station_reviews
        where
        rating IN
        <foreach collection="dictValueList" item="dictValue" open="(" separator="," close=")">
            #{dictValue}
        </foreach>
        AND charging_station_id = #{stationId}
        AND del_flag = '0'
        group by rating
    </select>

</mapper>